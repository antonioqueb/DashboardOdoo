<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!--   Define una acción a ejecutar  ..  . -->
    <record id="action_open_dasboard_dg" model="ir.actions.client">
        <field name="name">Dashboard Dirección General</field>
        <field name="tag">TobaccoMetricsPro.Dashboard</field>
    </record>

    <record id="action_open_dasboard_g" model="ir.actions.client">
        <field name="name">Dashboard Dirección General</field>
        <field name="tag">TobaccoMetricsPro.Dashboard</field>
    </record>

    <menuitem id="menu_dashboard"
              name="Tobacco Metrics Pro"
              action="action_open_dasboard_dg"
              sequence="20"/>

    <menuitem id="menu_dashboard_gerentes"
              name="Dashboard Gerentes"
              action="action_open_dasboard_g"
              sequence="20"
              parent="menu_dashboard"
              />
</odoo>
